#include <stdio.h>
int main()
{     
    int i;
	printf(" Count from 5 to 15:\n");
	for (i=5;i<=15;i++)
	{      
		printf("%d\n ",i);
	}
    return 0;
}




/*CODE (WHILE LOOP)*/
#include <stdio.h>
int main()
{
    int i;

   
    i=5;
    while(i<=15)
    {
        printf("%d\n", i);
        i++;
    }

    return 0;
}




/*CODE (DO While Loop)*/
#include<stdio.h>
int main()
{
	int i;

	do
	{
printf("%d\n", i);
		i++;
	}while(i <= 15);

	printf("\n");
}

